{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "outputs": ["dist/**"]
        },
        "clean": {},
        "test": {},
        "lint": { "outputs": [] },
        "lint:staged": { "outputs": [] },
        "lint:changed": { "outputs": [] },
